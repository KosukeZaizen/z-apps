  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "watch": {
    "typescriptCompile": {
      "patterns": [
        "origin"
      ],
      "extensions": "css,png,jpg,jpeg,ts,tsx",
      "quiet": false
    }
  },
  "scripts": {
    "start": "start npm run watch & (rimraf ./build && react-scripts start)",
    "build": "react-scripts build tsc",
    "typescriptCompile": "(copyfiles -u 1 origin/**/*.html origin/**/*.css origin/**/*.png origin/**/*.jpg origin/**/*.jpeg src/) && tsc",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "watch": "tsc & npm-watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
